{
  "expo": {
    "name": "pictionis",
    "slug": "pictionis",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/flame.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#F57C00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "martin.pictionis"
    },
    "android": {
      "package": "martin.pictionis"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}